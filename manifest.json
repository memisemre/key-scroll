{
    "manifest_version": 3,
    "name": "Key Scroll",
    "version": "0.0.1",
    "description": "Key Scroll",
    "action": {
      "default_popup": "popup/popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["options/options.js"]
      }
    ],
    "commands": {
        "_execute_action": {
          "suggested_key": {
            "default": "Ctrl+B",
            "mac": "Command+B"
          }
        }
    },
    "minimum_chrome_version": "88",
    "icons": {
      "16": "icons/16.png",
      "32": "icons/32.png",
      "64": "icons/64.png",
      "128": "icons/128.png"
    }
}